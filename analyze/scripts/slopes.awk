/bulkWrite\(0x100[01][048c]000,/ {
	slope=0
	if(substr($1,16,2)=="00")
		slope=1
	if(substr($1,16,2)=="04")
		slope=2
	if(substr($1,16,2)=="08")
		slope=3
	if(substr($1,16,2)=="0c")
		slope=4
	if(substr($1,16,2)=="10")
		slope=5
	# shading ?
	if(slope==0)
	{
		print $0
		next;
	}
	cnt=NF/2
	printf "write slope %d (%d)=",slope,cnt
	for(i=0;i<cnt;i++)
	{
		if(i==0)
		{
			val=strtonum(substr($1,27))
		}
		else
		{
			val=strtonum($(i*2+1))
		}
		val+=strtonum($(i*2+2))*256
		if(i==0)
		{
			printf "%d",val
		}
		else
		{
			printf ",%d",val
		}
	}
	printf "\n"
	next
}
